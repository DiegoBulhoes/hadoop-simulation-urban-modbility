python "$SUMO_HOME/tools/randomTrips.py" -n osm.net.xml --fringe-factor 1 -p 0.601163 -o osm.pedestrian.trips.xml -e 150 -r osm.pedestrian.rou.xml --vehicle-class pedestrian --pedestrians --prefix ped --max-distance 2000
